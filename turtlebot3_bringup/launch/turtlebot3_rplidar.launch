<launch>
  <arg name="set_frame_id" default="base_scan"/>
  <arg name="anglecomp" default="false"/>
  <arg name="rplidarmodel" default="$(env rplidar_model)"/>
  <arg name="serbaudrate" default="$(env rplidar_baudrate)"/>

  <node name="rplidarNode" pkg="rplidar_ros" type="rplidarNode" output="screen">
    <param name="serial_port"  type="string" value="/dev/ttyUSB0"/>
    <param name="serial_baudrate" type="int" value="$(arg serbaudrate)"/>
    <param name="frame_id" type="string" value="$(arg set_frame_id)"/>
    <param name="inverted" type="bool" value="false"/>
    <param name="angle_compensated" type="bool" value="$(arg anglecomp)"/>
  </node>
</launch>
